{
    "name": "mootaco/moota-woocommerce",
    "description": "moota.co plugin for WooCommerce",
    "type": "wordpress-plugin",
    "license": "GPL-2.0+-only",
    "minimum-stability": "stable",
    "config": {
        "vendor-dir": "lib/vendor"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/mootaco/php-sdk.git"
        }
    ],
    "require": {
        "php": ">= 5.3.0",
        "moota/moota-sdk": "1.1.*"
    },
    "autoload": {
        "files": ["functions.php"],
        "psr-4": {
            "Moota\\Woocommerce\\": "lib/woocommerce"
        }
    }
}
